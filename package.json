{
  "name": "wdm",
  "version": "1.0.0",
  "description": "Web Download Manager",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "env DEVELOPMENT=true nodemon server.js",
    "build": "npm run sass-build && npm run webpack-build",
    "webpack-watch": "webpack --watch",
    "webpack-build": "webpack --mode production",
    "sass-watch": "sass sass/style.scss public/stylesheets/style.min.css -s compressed --no-source-map --watch",
    "sass-build": "sass sass/style.scss public/stylesheets/style.min.css -s compressed --no-source-map"
  },
  "keywords": [
    "Download",
    "Manager",
    "WebApp"
  ],
  "author": "Husam Ahmed <husam999h@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@ceicc/range": "^3.0.0-beta.1",
    "axios": "^0.26.1",
    "express": "^4.17.3",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.51.0",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2"
  }
}
